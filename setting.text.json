  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "No italics",
        "scope": [
          "comment",
          "keyword",
          "variable.language",
          "meta.function-call",
          "meta.block",
          "storage",
          "entity.name.type",
          "entity.other.attribute-name",
          "keyword.control.import",
          "keyword.control.from",
          "keyword.control.flow",
          "keyword.control.export",
          "keyword.control.default",
          "storage.type.function",
          "storage.modifier.async",
          "storage.type.type",
          "storage.type",
          "meta.var.expr storage.type",
          "storage.modifier",
          "keyword.operator.new",
          "keyword.operator.delete",
          "storage.modifier.static",
          "storage.modifier.private",
          "storage.modifier.protected",
          "keyword.control.conditional",
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.ts",
          "entity.other.attribute-name.jsx",
          "entity.other.attribute-name.tsx",
          "comment.line.double-slash",
          "comment.line.double-slash.documentation",
          "variable.parameter",
          "punctuation",
          "meta.brace.round",
          "punctuation.definition.block",
          "keyword.control.as"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "punctuation.definition.tag.begin",
          "punctuation.definition.tag.end",
          "meta.tag",
          "punctuation",
          "meta.brace.round",
          "source",
          "text.html.derivative",
          "keyword.operator.comparison",
          "keyword.operator.ternary",
          "keyword.operator.assignment",
          "keyword.operator.logical",
          "keyword.operator.relational",
          "keyword.operator.arithmetic",
          "keyword.operator.type.annotation",
          "punctuation.separator.key-value",
          "keyword.operator.type",
          "meta.brace.square",
          "meta.arrow",
          "storage.type.function.arrow",
          "punctuation.section.embedded.begin",
          "punctuation.section.embedded.end",
          "meta.jsx.children",
          "storage.type.function.arrow"
        ],
        "settings": {
          "foreground": "#dddddd"
        }
      },
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#a2a2a2"
        }
      }
    ],
    "[GitHub Dark Default]": {
      "textMateRules": [
        {
          "scope": [
            "punctuation.terminator.rule.",
            "punctuation.accessor",
            "punctuation.definition.template-expression",
            "punctuation.separator.key-value",
            "punctuation.separator.comma",
            "punctuation.terminator.statement",
            "keyword.operator.assignment.compound",
            "keyword.operator.assignment",
            "meta.type.annotation",
            "keyword.operator.comparison",
            "meta.embedded.expression",
            "keyword.operator.ternary",
            "keyword.operator.arithmetic",
            "keyword.operator.logical",
            "punctuation.section.embedded.begin",
            "punctuation.section.embedded.end",
            // "punctuation.definition.block",
            "storage.type.function.arrow",
            "keyword.operator.type",
            "keyword.operator.type.annotation",
            "punctuation.definition.typeparameters.begin",
            "punctuation.definition.typeparameters.end"
            // "meta.brace.square"
          ],
          "settings": {
            "foreground": "#F97583",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "variable.other.object",
            "variable.object.property",
            "variable.other.readwrite"
            // "meta.object-literal.key"
          ],
          "settings": {
            "foreground": "#79C0FF"
          }
        },
        {
          "scope": [
            "support.type.property-name.css",
            "variable.other.object.property",
            "variable.other.readwrite.alias"
            // "constant.numeric.decimal"
          ],
          "settings": {
            "foreground": "#fafafa"
          }
        }
      ]
    },
    "[Min Dark]": {
      "textMateRules": [
        {
          "scope": [
            "entity.other.attribute-name.class.css",
            "entity.name.tag",
            "support.type.property-name.json.comments"
            // "support.class.component"
          ],
          "settings": {
            "foreground": "#7EE787"
          }
        },
        {
          "scope": ["string.quoted", "string.template"],
          "settings": {
            "foreground": "#A5D6FF"
          }
        },
        {
          "scope": [
            "variable.parameter",
            "source.tsx meta.function.tsx meta.block.tsx",
            "meta.var.expr.tsx meta.objectliteral.tsx",
            "variable.other.readwrite"
