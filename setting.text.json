  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "No italics",
        "scope": [
          "comment",
          "keyword",
          "variable.language",
          "meta.function-call",
          "meta.block",
          "storage",
          "entity.name.type",
          "entity.other.attribute-name",
          "keyword.control.import",
          "keyword.control.from",
          "keyword.control.flow",
          "keyword.control.export",
          "keyword.control.default",
          "storage.type.function",
          "storage.modifier.async",
          "storage.type.type",
          "storage.type",
          "meta.var.expr storage.type",
          "storage.modifier",
          "keyword.operator.new",
          "keyword.operator.delete",
          "storage.modifier.static",
          "storage.modifier.private",
          "storage.modifier.protected",
          "keyword.control.conditional",
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.ts",
          "entity.other.attribute-name.jsx",
          "entity.other.attribute-name.tsx",
          "comment.line.double-slash",
          "comment.line.double-slash.documentation",
          "variable.parameter",
          "punctuation",
          "meta.brace.round",
          "punctuation.definition.block"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "punctuation.definition.tag.begin",
          "punctuation.definition.tag.end",
          "meta.tag",
          "punctuation",
          "meta.brace.round",
          "source",
          "text.html.derivative",
          "keyword.operator.comparison",
          "keyword.operator.ternary",
          "meta.brace.square",
          "keyword.operator.assignment",
          "keyword.operator.type.annotation",
          "storage.type.function.arrow",
          "keyword.operator.arithmetic",
          "punctuation.separator.key-value",
          "punctuation.section.embedded.begin",
          "punctuation.section.embedded.end"
        ],
        "settings": {
          "foreground": "#ededed"
        }
      },
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#a7a4a4"
        }
      }
    ],
    "[GitHub Dark Default]": {
      "textMateRules": [
        {
          "scope": ["entity.other.attribute-name.class.css"],
          "settings": {
            "foreground": "#7EE787"
          }
        },
        {
          "scope": [
            "support.constant.property-value.css",
            "support.function.misc.css",
            "constant.numeric.css",
            "support.constant.color.w3c-extended-color-name.css",
            "string.quoted.single.css",
            "string.quoted.double.css",
            "support.constant.font-name.css",
            "punctuation.definition.tag.begin",
            "punctuation.definition.tag.end",
            "keyword.operator.assignment",
            "variable.object.property",
            "support.variable.property",
            "variable.other.constant.property",
            "punctuation.definition.tag",
            "punctuation.definition.typeparameters",
            "variable.other.property",
            "support.constant.property"
          ],
          "settings": {
            "foreground": "#fafafa"
          }
        },
        {
          "scope": [
            "entity.other.attribute-name.pseudo-element.css",
            "support.function.gradient.css"
          ],
          "settings": {
            "foreground": "#d2a8ff"
          }
        },
        {
          "scope": ["comment", "punctuation.definition.comment"],
          "settings": {
            "foreground": "#b2b2b3"
          }
        }
      ]
    },
    "[Min Theme+]": {
      "textMateRules": [
        {
          "scope": [
            "support.type.primitive.tsx",
            "support.type.primitive.jsx",
            "support.type.primitive.js",
            "support.type.primitive.ts",
            "entity.name.type"
          ],
          "settings": {
            "foreground": "#D19A66"
          }
        },
        {
          "scope": ["support.type.property-name"],
          "settings": {
            "foreground": "#e9e8e8"
          }
        },
        {
          "scope": ["source.tsx meta.export.default.tsx variable.other.readwrite.tsx"],
          "settings": {
            "foreground": "#E5C07B"
          }
        },
        {
          "scope": "source.tsx meta.export.default.tsx variable.other.readwrite.tsx",
          "settings": {
            "foreground": "#E06C75"
          }
        }
      ]
    }
  },
