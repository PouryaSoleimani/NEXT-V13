          "settings": {
            // "foreground": "#63cad7"
            "foreground": "#D19A66"
          }
        },
        {
          "scope": ["support.type.property-name"],
          "settings": {
            "foreground": "#e9e8e8"
          }
        },
        {
          "scope": [
            "source.tsx meta.export.default.tsx variable.other.readwrite.tsx",
            "meta.function.tsx",
            "source.tsx meta.function.tsx meta.block.tsx meta.var.expr.tsx meta.objectliteral.tsx meta.object.member.tsx meta.objectliteral.tsx meta.object.member.tsx meta.object-literal.key.tsx",
            "source.tsx meta.var.expr.tsx meta.arrow.tsx meta.block.tsx meta.var.expr.tsx meta.objectliteral.tsx meta.object.member.tsx meta.objectliteral.tsx meta.object.member.tsx meta.object-literal.key.tsx"
