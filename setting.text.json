  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "No italics",
        "scope": ["comment", "keyword", "variable.language", "meta.function-call", "meta.block", "storage", "entity.name.type", "entity.other.attribute-name", "keyword.control.import", "keyword.control.from", "keyword.control.flow", "keyword.control.export", "keyword.control.default", "storage.type.function", "storage.modifier.async", "storage.type.type", "storage.type", "meta.var.expr storage.type", "storage.modifier", "keyword.operator.new", "keyword.operator.delete", "storage.modifier.static", "storage.modifier.private", "storage.modifier.protected", "keyword.control.conditional", "entity.other.attribute-name.js", "entity.other.attribute-name.ts", "entity.other.attribute-name.jsx", "entity.other.attribute-name.tsx", "comment.line.double-slash", "comment.line.double-slash.documentation", "variable.parameter", "punctuation", "meta.brace.round", "punctuation.definition.block"],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": ["punctuation.definition.tag.begin", "punctuation.definition.tag.end", "meta.tag", "punctuation", "meta.brace.round", "source", "text.html.derivative", "keyword.operator.comparison", "keyword.operator.ternary", "meta.brace.square", "keyword.operator.assignment", "keyword.operator.type.annotation", "keyword.operator.type", "keyword.operator.spread"],
        "settings": {
          "foreground": "#fff"
        }
      },
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#909090"
        }
      }
    ],
    "[GitHub Dark Default]": {
      "textMateRules": [
        {
          "scope": ["entity.other.attribute-name.class.css"],
          "settings": {
            "foreground": "#7EE787"
          }
        },
        {
          "scope": ["support.constant.property-value.css", "support.function.misc.css", "constant.numeric.css", "support.constant.color.w3c-extended-color-name.css", "string.quoted.single.css", "string.quoted.double.css", "support.constant.font-name.css", "punctuation.definition.tag.begin", "punctuation.definition.tag.end", "keyword.operator.assignment", "variable.object.property", "support.variable.property", "variable.other.constant.property", "punctuation.definition.tag", "punctuation.definition.typeparameters", "variable.other.property", "support.constant.property"],
          "settings": {
            "foreground": "#fafafa"
          }
        },
        {
          "scope": ["entity.other.attribute-name.pseudo-element.css", "support.function.gradient.css", "entity.other.attribute-name", "entity.name.function"],
          "settings": {
            "foreground": "#b392f0"
          }
        },
        {
          "scope": ["punctuation.terminator.rule.", "punctuation.accessor", "punctuation.definition.template-expression", "punctuation.separator.key-value", "punctuation.separator.comma", "punctuation.terminator.statement", "keyword.operator.assignment.compound", "keyword.operator.assignment", "meta.type.annotation"],
          "settings": {
            "foreground": "#F97583",
            "fontStyle": "bold"
          }
        },
        {
          "scope": ["support.class.component", "meta.object.member"],
          "settings": {
            "foreground": "#79B8FF"
          }
        },
        {
          "scope": ["comment", "punctuation.definition.comment"],
          "settings": {
            "foreground": "#b2b2b3"
          }
        },
        {
          "scope": ["entity.name.type.module", "entity.name.type"],
          "settings": {
            "foreground": "#f69d50"
          }
        }
      ]
    },
    "[Min Dark]": {
      "textMateRules": [
        {
          "scope": ["entity.other.attribute-name.class.css", "entity.name.tag", "support.type.property-name.json.comments", "support.class.component"],
          "settings": {
            "foreground": "#7EE787"
          }
        },
        {
          "scope": ["string.quoted", "string.template"],
          "settings": {
            "foreground": "#A5D6FF"
          }
        },
        {
          "scope": ["support.constant.property-value.css", "support.function.misc.css", "constant.numeric.css", "support.constant.color.w3c-extended-color-name.css", "string.quoted.single.css", "string.quoted.double.css", "support.constant.font-name.css", "punctuation.definition.tag.begin", "punctuation.definition.tag.end", "keyword.operator.assignment", "variable.object.property", "support.variable.property", "variable.other.constant.property", "punctuation.definition.tag", "punctuation.definition.typeparameters", "variable.other.property", "support.constant.property", "meta.object-literal.key", "variable.other.object.property", "entity.name.label", "meta.namespace.declaration"],
          "settings": {
            "foreground": "#fafafa"
          }
        },
        {
          "scope": ["entity.other.attribute-name.pseudo-element.css", "support.function.gradient.css", "entity.other.attribute-name", "entity.name.function"],
          "settings": {
            "foreground": "#b392f0"
          }
        },
        {
          "scope": ["punctuation.terminator.rule.", "punctuation.accessor", "punctuation.definition.template-expression", "punctuation.separator.key-value", "punctuation.separator.comma", "punctuation.terminator.statement", "keyword.operator.assignment.compound", "keyword.operator.assignment", "meta.type.annotation", "keyword.operator.comparison", "meta.embedded.expression", "keyword.operator.ternary"],
          "settings": {
            "foreground": "#F97583",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [
            "support.class.component",
            "meta.object.member",
            "variable.other.object"
          ],
          "settings": {
            "foreground": "#79B8FF"
          }
        },
        {
          "scope": ["comment", "punctuation.definition.comment"],
          "settings": {
            "foreground": "#a7a6a6"
          }
        },
        {
          "scope": [
            "entity.name.type.module",
            "entity.name.type",
            "support.type.primitive"
          ],
          "settings": {
            "foreground": "#f69d50",
            "fontStyle": ""
          }
        }
      ]
    }
  },
