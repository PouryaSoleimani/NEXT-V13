  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "No italics",
        "scope": [
          "comment",
          "keyword",
          "variable.language",
          "meta.function-call",
          "meta.block",
          "storage",
          "entity.name.type",
          "entity.other.attribute-name",
          "keyword.control.import",
          "keyword.control.from",
          "keyword.control.flow",
          "keyword.control.export",
          "keyword.control.default",
          "storage.type.function",
          "storage.modifier.async",
          "storage.type.type",
          "storage.type",
          "meta.var.expr storage.type",
          "storage.modifier",
          "keyword.operator.new",
          "keyword.operator.delete",
          "storage.modifier.static",
          "storage.modifier.private",
          "storage.modifier.protected",
          "keyword.control.conditional",
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.ts",
          "entity.other.attribute-name.jsx",
          "entity.other.attribute-name.tsx",
          "comment.line.double-slash",
          "comment.line.double-slash.documentation",
          "variable.parameter",
          "punctuation",
          "meta.brace.round",
          "punctuation.definition.block"
        ],
        "settings": {
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "punctuation.definition.tag.begin",
          "punctuation.definition.tag.end",
          "meta.tag",
          "support.type.property-name.css",
          "punctuation",
          "meta.brace.round",
          "source",
          "text.html.derivative"
        ],
        "settings": {
          "foreground": "#fff"
        }
      },
      {
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "foreground": "#787878"
        }
      }
    ],
    "[GitHub Dark Default]": {
      "textMateRules": [
        {
          "scope": ["entity.other.attribute-name.class.css"],
          "settings": {
            "foreground": "#7EE787"
          }
        },
        {
          "scope": [
            "meta.array-binding-pattern-variable",
            "punctuation.section.embedded",
            "punctuation.definition.template-expression.begin",
            "punctuation.definition.template-expression.end"
          ],
          "settings": {
            "foreground": "#eee350"
          }
        },
        {
          "scope": [
            "support.constant.property-value.css",
            "support.function.misc.css",
            "constant.numeric.css",
            "support.constant.color.w3c-extended-color-name.css",
            "string.quoted.single.css",
            "string.quoted.double.css",
            "support.constant.font-name.css",
            "punctuation.definition.tag.begin",
            "punctuation.definition.tag.end",
            "variable.object.property",
            "support.variable.property",
            "support.constant.color.w3c-standard-color-name.css"
          ],
          "settings": {
            "foreground": "#fafafa"
          }
        },
        {
          "scope": [
            "entity.other.attribute-name.pseudo-element.css",
            "support.function.gradient.css",
            "entity.other.attribute-name",
            "entity.name.function"
          ],
          "settings": {
            "foreground": "#B392F0"
          }
        },
        {
          "scope": [
            "punctuation.terminator.rule.",
            "punctuation.accessor",
            "punctuation.definition.template-expression",
            "punctuation.separator.key-value",
            "punctuation.separator.comma",
            "punctuation.terminator.statement",
            "keyword.operator.assignment.compound"
          ],
          "settings": {
            "foreground": "#FF7B72",
            "fontStyle": "bold"
          }
        },
        {
          "scope": [],
          "settings": {
            "foreground": "#ffa657"
          }
        },
        {
          "scope": [
            "support.class.component",
            "support.type.property-name.css"
          ],
          "settings": {
            "foreground": "#79B8FF"
          }
        },
        {
          "scope": ["comment", "punctuation.definition.comment"],
          "settings": {
            "foreground": "#b2b2b3"
          }
        }
      ]
    },
    "[Min Dark]": {
      "textMateRules": [
        {
          "scope": [
            "meta.object-literal.key",
            "source.tsx",
            "support.class.component"
          ],
          "settings": {
            "foreground": "#79B8FF"
          }
        },
        {
          "scope": ["constant.numeric.css", "variable.other.readwrite.alias"],
          "settings": {
            "foreground": "#fff"
          }
        },
        {
          "scope": [
            "punctuation.terminator.rule.",
